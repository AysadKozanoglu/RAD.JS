<transition id="animation-group"
            class="<%= data.props.direction %>"
            initialAnimation="none"
            animationEnter="<%= data.props.animationEnter %>"
            animationLeave="<%= data.props.animationLeave %>"
            enterTimeout="<%= data.props.enterTimeout || 1000 %>"
            leaveTimeout="<%= data.props.leaveTimeout || 1000 %>" >

    <% if (data.props.direction === 'forward') { %>
    <Details key="details" />
    <% } else { %>
    <List key="transitions-list" />
    <% } %>

</transition>